{
  "title": "Export works CSV (Bloomen API)",
  "blocks": [
    {
      "type": "init"
    },
    {
      "type": "context",
      "contextPath": "jwt",
      "contextBlocks": [
        {
          "type": "variable-get",
          "name": "bloomen-jwt"
        }
      ],
      "blocks": [
        {
          "type": "http",
          "method": "get",
          "endpoint": {
            "protocol": "https:",
            "host": "bloomen.herokuapp.com",
            "pathname": "/sound/music"
          },
          "authentication": {
            "type": "bearer",
            "valueGetters": {
              "jwt": "context.jwt"
            }
          }
        }
      ]
    },
    {
      "type": "mapping",
      "mapping": "data[].{ id: id, title: originalTitle, creators: creators[].name, alternativeTitles: alternativeTitles, performers: associatedPerformers, isrcs: associatedISRCs, ISWC: ISWC }"
    },
    {
      "type": "csv-export",
      "header": true,
      "skipEmptyLines": true
    }
  ]
}
